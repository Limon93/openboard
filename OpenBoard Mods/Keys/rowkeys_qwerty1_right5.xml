<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
    >
    <switch>
        <case
            latin:numberRowEnabled="true">
            <Key
                latin:keySpec="!text/keyspec_y"
                latin:keyHintLabel="]"
                latin:additionalMoreKeys="]"
                latin:moreKeys="!text/morekeys_y"/>
            <!-- U+003C: "<" LESS-THAN SIGN -->
            <Key
                latin:keySpec="u"
                latin:keyHintLabel="&#x00F9;"
                latin:additionalMoreKeys="&#x00F9;"
                latin:moreKeys="!text/morekeys_u"/>
            <!-- U+003E: ">" GREATER-THAN SIGN -->
            <Key
                latin:keySpec="i"
                latin:keyHintLabel="&#x00EC;"
                latin:additionalMoreKeys="&#x00EC;"
                latin:moreKeys="!text/morekeys_i"/>
            <Key
                latin:keySpec="o"
                latin:keyHintLabel="&#x00F2;"
                latin:additionalMoreKeys="&#x00F2;"
                latin:moreKeys="!text/morekeys_o"/>
            <Key
                latin:keySpec="p"
                latin:keyHintLabel="}"
                latin:additionalMoreKeys="}"/>

        </case>

        <default>
            <Key
                latin:keySpec="!text/keyspec_y"
                latin:keyHintLabel="6"
                latin:additionalMoreKeys="6"
                latin:moreKeys="!text/morekeys_y"/>
            <Key
                latin:keySpec="u"
                latin:keyHintLabel="&#x00F9;"
                latin:additionalMoreKeys="&#x00F9;"
                latin:moreKeys="!text/morekeys_u"/>
            <Key
                latin:keySpec="i"
                latin:keyHintLabel="&#x00EC;"
                latin:additionalMoreKeys="&#x00EC;"
                latin:moreKeys="!text/morekeys_i"/>
            <Key
                latin:keySpec="o"
                latin:keyHintLabel="&#x00F2;"
                latin:additionalMoreKeys="&#x00F2;"
                latin:moreKeys="!text/morekeys_o"/>
            <Key
                latin:keySpec="p"
                latin:keyHintLabel="0"
                latin:additionalMoreKeys="0"/>
        </default>
    </switch>
</merge>
